dni_letter = id_number.name[
    len(id_number.name) - 1 : len(id_number.name)
]
if dni_number.isdigit() and not dni_letter.isdigit():
    if letters.get(int(dni_number) % 23) != dni_letter.upper():
        failed = True
else:
    failed = True
