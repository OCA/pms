<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <data noupdate="1">
        <record id="cron_connector_pms_wubook_export" model="ir.cron">
            <field name="name">PMS Connector Wubook Exports</field>
            <field name="active" eval="False" />
            <field name="interval_number">1</field>
            <field name="user_id" ref="base.user_root" />
            <field name="interval_type">minutes</field>
            <field name="numbercall">-1</field>
            <field name="doall" eval="False" />
            <field name="state">code</field>
            <field name="model_id" ref="model_channel_wubook_backend" />
            <field
                name="nextcall"
                eval="(DateTime.now() + timedelta(days=1)).strftime('%Y-%m-%d 09:00:00')"
            />
            <field name="code">model._scheduler_export(interval=1, count=10)</field>
        </record>
    </data>
</odoo>
