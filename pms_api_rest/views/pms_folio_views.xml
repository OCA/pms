<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <record model="ir.ui.view" id="inherit_pms_folio_view_form">
        <field name="name">pms.folio.form</field>
        <field name="model">pms.folio</field>
        <field name="inherit_id" ref="pms.pms_folio_view_form" />
        <field name="arch" type="xml">
            <xpath
                expr="//page[.//field[@name='force_nothing_to_invoice']]/group[last()]"
                position="after"
            >
                <group>
                    <field name="api_rest_id" />
                </group>
            </xpath>
        </field>
    </record>

</odoo>
