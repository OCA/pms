        <field
            name="availability_rule_field_ids"
            eval="[(6, 0, [ref('pms.field_pms_availability_plan_rule__min_stay'), ref('pms.field_pms_availability_plan_rule__quota')])]"
        />
    </record>
    <record id="base.user_demo" model="res.users">
