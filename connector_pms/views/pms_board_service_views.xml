<odoo>
    <record id="pms_board_service_form" model="ir.ui.view">
        <field name="name">pms.board.service.connector.form</field>
        <field name="model">pms.board.service</field>
        <field name="inherit_id" ref="pms.pms_board_service_form" />
        <field
            name="groups_id"
            eval="[(4, ref('connector.group_connector_manager'))]"
        />
        <field name="arch" type="xml">
            <xpath expr="/form">
                <!-- change the invisible attribute to 0 when used
                     in submodules -->
                <notebook>
                    <page string="Connectors" name="connector" />
                </notebook>
            </xpath>
        </field>
    </record>
</odoo>
