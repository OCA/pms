        <field name="inherit_id" ref="hotel.reservation_restriction_item_view_form" />
        <field name="arch" type="xml">
            <xpath expr="//field[@name='date_end']" position="attributes">
                <attribute name="readonly">True</attribute>
            </xpath>
        </field>
    </record>

</odoo>
