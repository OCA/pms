<odoo>
    <template id="property_casai_website" name="Property Casai Website" track="1">
        <t name="Property" track="1" t-name="pms_website.property">
            <t t-call="website.layout">
                <t t-set="additional_title" t-value="property.name" />
                <div
                    class="oe_structure oe_empty"
                    id="oe_structure_website_sale_property_1"
                />
                <t t-call="connector_guesty.pms_website_header_01" />
                <t t-call="connector_guesty.website_info_bar_01_template" />
                <t t-call="connector_guesty.website_abot_apt_template" />
                <t t-call="connector_guesty.website_section_policies_template" />
                <t t-call="connector_guesty.website_section_location_template" />
                <!--                <t t-call="connector_guesty.website_dynamic_reviews_template"/>-->
                <t t-call="connector_guesty.website_fixed_amenities_template" />
                <t t-call="connector_guesty.pms_website_dynamic_amenities_template" />
            </t>
        </t>
    </template>
</odoo>
